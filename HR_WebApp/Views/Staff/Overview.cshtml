@model IEnumerable<StaffListViewModel>;

    <h1>Staff</h1>

    <a asp-controller="Staff" asp-action="Ĉreate">Personeelslid toevoegen</a>

    <table>
        <thead>
            <tr>
                <th>Voornaam</th>
                <th>Achternaam</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                </tr>
            }
        </tbody>
    </table>